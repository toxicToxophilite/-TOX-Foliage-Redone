<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="DeciduousTreeBase">
    <defName>Tox_Amastani</defName>
    <label>Amastani Tree</label>
    <description>Amastani trees are large, gnarled, and otherwordly. Because of the gnarled wood, it takes a long time to chop these trees down, but they yield a lot of wood to make the time worth the effort.</description>
    <graphicData>
      <texPath>Plants/TreeAmastani/Mature</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.2, 0.35, 0.13)</volume>
        <offset>(-0.03,0,-0.44)</offset>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>8</Beauty>
      <MaxHitPoints>400</MaxHitPoints>
      <Flammability>0.4</Flammability>
      <Nutrition>3</Nutrition>

    </statBases>
		<selectable>true</selectable>
		<pathCost>10</pathCost>
    <plant>
			<fertilityMin>0.2</fertilityMin>
			<fertilitySensitivity>0.1</fertilitySensitivity>
			<growDays>50</growDays>
      <harvestWork>5000</harvestWork>
      <harvestYield>190</harvestYield>
      <wildClusterWeight>35</wildClusterWeight>
      <leaflessGraphicPath>Plants/TreeAmastani/Leafless</leaflessGraphicPath>
      <visualSizeRange>6~7</visualSizeRange>
      <blockAdjacentSow>true</blockAdjacentSow>
    </plant>
  </ThingDef>





</Defs>

<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TallGrass"]/graphicData</xpath>
	  <value>
	    <graphicData>
		  <texPath>Plants/TallGrass</texPath>
		  <graphicClass>Graphic_Random</graphicClass>
		</graphicData>
	  </value>
	</Operation>

<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TallGrass"]/statBases</xpath>
	  <value>
	    <statBases>
      		<MaxHitPoints>90</MaxHitPoints>
      		<Flammability>1.3</Flammability>
      		<Nutrition>0.30</Nutrition>
      		<Beauty>2</Beauty>
    	    </statBases>
	  </value>
	</Operation>

<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TallGrass"]/plant</xpath>
	  <value>
	    <plant>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <growDays>3</growDays>
      <sowTags>
	<li>Ground</li>
      </sowTags>
      <harvestedThingDef>Hay</harvestedThingDef>
	<harvestTag>Hay</harvestTag>
	<harvestYield>14</harvestYield>
      <maxMeshCount>9</maxMeshCount>
      <visualSizeRange>0.7~0.85</visualSizeRange>
      <topWindExposure>0.4</topWindExposure>
      <wildClusterRadius>4</wildClusterRadius>
      <wildClusterWeight>10</wildClusterWeight>
      <wildOrder>1</wildOrder>
    </plant>
	  </value>
	</Operation>


</Patch>
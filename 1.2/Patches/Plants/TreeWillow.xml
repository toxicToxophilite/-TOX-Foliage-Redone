<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TreeWillow"]/graphicData</xpath>
	  <value>
	    <graphicData>
		  <texPath>Plants/TreeWillow/Mature</texPath>
      		  <graphicClass>Graphic_Random</graphicClass>
      		  <shadowData>
        		<volume>(0.2, 0.35, 0.13)</volume>
        		<offset>(-0.03,0,-0.44)</offset>
      		  </shadowData>
		</graphicData>
	  </value>
	</Operation>

<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TreeWillow"]/statBases</xpath>
	  <value>
	    <statBases>
      <Beauty>2</Beauty>
    	    </statBases>
	  </value>
	</Operation>

<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TreeWillow"]/plant</xpath>
	  <value>
	    <plant>
      		<leaflessGraphicPath>Plants/TreeWillow/Leafless</leaflessGraphicPath>
      		<growDays>13</growDays>
      		<harvestWork>800</harvestWork>
      		<harvestYield>27</harvestYield>
      		<blockAdjacentSow>true</blockAdjacentSow>
      		<visualSizeRange>4.5~5</visualSizeRange>
    	    </plant>
	  </value>
	</Operation>


</Patch>
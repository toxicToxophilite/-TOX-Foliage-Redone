<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_Cocoa"]/graphicData</xpath>
	  <value>
	    <graphicData>
		  <texPath>Plants/TreeCocoa/Mature</texPath>
		  <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
          <volume>(0.15, 0.3, 0.15)</volume>
          <offset>(0,0,-0.38)</offset>
      </shadowData>
		</graphicData>
	  </value>
	</Operation>


<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_TreeCocoa"]/plant</xpath>
	  <value>
<plant>
      <dieIfLeafless>true</dieIfLeafless>
      <growDays>16</growDays>
      <visualSizeRange>2.8~4.0</visualSizeRange>
      <sowMinSkill>8</sowMinSkill>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <sowResearchPrerequisites Inherit="False">
        <li>Cocoa</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.15</topWindExposure>
      <mustBeWildToSow>false</mustBeWildToSow>
      <immatureGraphicPath>Plants/TreeCocoa/Immature</immatureGraphicPath>
      <harvestWork>400</harvestWork>
      <harvestTag>Standard</harvestTag>
      <harvestYield>20</harvestYield>
      <harvestedThingDef>Chocolate</harvestedThingDef>
      <harvestFailable>true</harvestFailable>
</plant>
	  </value>
	</Operation>



</Patch>
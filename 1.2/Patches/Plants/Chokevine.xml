<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_Chokevine"]/graphicData</xpath>
	  <value>
	    <graphicData>
		  <texPath>Plants/Chokevine/Mature</texPath>
		  <graphicClass>Graphic_Random</graphicClass>
			<drawSize>1.1</drawSize>
			<shadowData>
				<volume>(0.3, 0.2, 0.15)</volume>
				<offset>(0,0,-0.1)</offset>
			</shadowData>
		</graphicData>
	  </value>
	</Operation>

<Operation Class="PatchOperationReplace">
	  <xpath>*/ThingDef[defName = "Plant_Chokevine"]/plant</xpath>
	  <value>
	    <plant>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <growDays>5</growDays>
      <harvestWork>150</harvestWork>
      <maxMeshCount>16</maxMeshCount>
      <leaflessGraphicPath>Plants/Chokevine/Leafless</leaflessGraphicPath>
      <visualSizeRange>0.3~0.6</visualSizeRange>
      <topWindExposure>0.16</topWindExposure>
      <wildClusterRadius>6</wildClusterRadius>
      <wildClusterWeight>200</wildClusterWeight>
      <wildOrder>1</wildOrder>
      <wildEqualLocalDistribution>false</wildEqualLocalDistribution>
	<sowTags>
		<li>Ground</li>
	</sowTags>
    </plant>
	  </value>
	</Operation>


</Patch>
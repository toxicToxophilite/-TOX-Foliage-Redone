<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
      <li Class="PatchOperationTest">
        <xpath>/Defs/BiomeDef[defName = "BiomesIslands_TropicalIsland"]</xpath>
      </li>
      <li Class="PatchOperationReplace">
        <!-- Changes to Animal Density for Tropical Rainforest -->
        <xpath>/Defs/BiomeDef[defName = "BiomesIslands_TropicalIsland"]/animalDensity</xpath>
        <value>
          <animalDensity>6.0</animalDensity>
        </value>
      </li>
      <li Class="PatchOperationReplace">
        <!-- Changes to Plant Density for TropicalRainforest Biome -->
        <xpath>/Defs/BiomeDef[defName = "BiomesIslands_TropicalIsland"]/plantDensity</xpath>
        <value>
          <plantDensity>3.0</plantDensity>
        </value>
      </li>
      <li Class="PatchOperationReplace">
        <!-- Changes to weather for TropicalRainforest Biome -->
        <xpath>/Defs/BiomeDef[defName = "BiomesIslands_TropicalIsland"]/baseWeatherCommonalities</xpath>
        <value>
          <baseWeatherCommonalities>
            <Clear>2</Clear>
            <Fog>5</Fog>
            <Rain>10</Rain>
            <DryThunderstorm>1.5</DryThunderstorm>
            <RainyThunderstorm>5.5</RainyThunderstorm>
            <FoggyRain>7</FoggyRain>
            <SnowGentle>1</SnowGentle>
          </baseWeatherCommonalities>
        </value>
      </li>
    </operations>
  </Operation>
</Patch>